{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Courier Prime",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/courier-prime_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Courier Prime",
					"slug": "courier-prime"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Alegreya SC",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/alegreya-sc_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Alegreya SC",
					"slug": "alegreya-sc"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ibarra-real-nova_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/ibarra-real-nova_normal_500.ttf"
							]
						}
					],
					"fontFamily": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				},
				{
					"fontFace": [
						{
							"fontFamily": "IM Fell English SC",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/im-fell-english-sc_normal_400.ttf"
							]
						}
					],
					"fontFamily": "IM Fell English SC",
					"slug": "im-fell-english-sc"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"h2": {
				"color": {
					"text": "#000000"
				}
			},
			"h4": {
				"color": {
					"text": "#000000"
				}
			},
			"heading": {
				"color": {
					"text": "#cf2e2e"
				},
				"typography": {
					"fontFamily": "IM Fell English SC"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--vivid-red)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}