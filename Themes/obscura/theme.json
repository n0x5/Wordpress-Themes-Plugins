{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "ABeeZee",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/abeezee_regular.ttf"
							]
						}
					],
					"fontFamily": "ABeeZee",
					"slug": "abeezee"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Anaheim",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/anaheim_regular.ttf"
							]
						}
					],
					"fontFamily": "Anaheim",
					"slug": "anaheim"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Black Ops One",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/black-ops-one_regular.ttf"
							]
						}
					],
					"fontFamily": "Black Ops One",
					"slug": "black-ops-one"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Caveat",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/caveat_regular.ttf"
							]
						}
					],
					"fontFamily": "Caveat",
					"slug": "caveat"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Cutive Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/cutive-mono_regular.ttf"
							]
						}
					],
					"fontFamily": "Cutive Mono",
					"slug": "cutive-mono"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/video": {
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				}
			}
		},
		"elements": {
			"heading": {
				"color": {
					"text": "#00d084"
				},
				"typography": {
					"fontFamily": "Cutive Mono",
					"letterSpacing": "3px",
					"lineHeight": "1.5",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|vivid-red"
				}
			}
		},
		"spacing": {
			"blockGap": "13px",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|anaheim",
			"lineHeight": "0"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}